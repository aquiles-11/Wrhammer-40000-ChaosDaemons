        </exclusionTags>
        <renderNodeProperties>
            <li>
                <workerClass>PawnRenderNodeWorker_AttachmentBody</workerClass>
                <texPath>Things/Pawn/Humanlike/Tzeentch/LordOfChange/BodyAttachments/Wings/LordOfChangeWings</texPath>
                <colorType>Custom</colorType>
                <overrideMeshSize>(2.3, 2.3)</overrideMeshSize>
                <parentTagDef>Body</parentTagDef>
                <useSkinShader>false</useSkinShader>
                <rotDrawMode>Fresh, Rotting</rotDrawMode>
				<drawData>
					<scaleOffsetByBodySize>true</scaleOffsetByBodySize>
					<defaultData>
						<layer>-2</layer>
					</defaultData>
					<dataNorth>
						<offset>(0, 0.2, 0.2)</offset>
						<layer>90</layer>
					</dataNorth>
					<dataSouth>
						<offset>(0, -0.2, 0.3)</offset>
					</dataSouth>
					<dataEast>
						<offset>(-0.7, 0.3, 0.3)</offset>
						<layer>90</layer>
					</dataEast>
					<dataWest>
						<offset>(0.7, 0.3, 0.3)</offset>
						<layer>90</layer>
					</dataWest>
				</drawData>
            </li>
        </renderNodeProperties>
    </GeneDef>
