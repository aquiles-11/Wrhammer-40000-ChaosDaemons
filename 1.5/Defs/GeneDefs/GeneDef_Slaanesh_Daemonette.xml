        <styleGender>Any</styleGender>
        <texPath>Things/Pawn/Humanlike/Slaanesh/Daemonette/HeadAttachments/Hair/DaemonetteHair2</texPath>
        <category>BEWH_Daemonette</category>
        <styleTags>
            <li>BEWH_DaemonetteHair</li>
            <li>Shaved</li>
            <li>Urban</li>
            <li>Punk</li>
            <li>Rural</li>
            <li>Tribal</li>
            <li>Soldier</li>
            <li>HairShort</li>
        </styleTags>
        <requiredGene>BEWH_DaemonetteHair</requiredGene>
    </HairDef>

  <StyleItemCategoryDef>
